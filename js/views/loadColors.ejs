<!DOCTYPE html>
<% include partials/head %>
<body class="container">
  <% include partials/header %>
  <main>
    Load colors
    <table border=1>
      <thead>
        <tr>
          <th>Colors</th>
        </tr>
      </thead>
      <tbody>
        <% for(var i = 0 ; i < colors.split(",").length ; i++) { %>
          <tr>
            <td style='background-color : #<%= colors.split(",")[i] %>'><%= colors.split(",")[i] %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </main>
  <% include partials/footer %>
</body>
</html>
